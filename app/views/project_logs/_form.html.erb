  <%= render 'shared/errors', obj: @project_log %>
  
  <%= form_for(:project_log, :html => {class: "form-horizontal", role: "form"}, url: project_project_logs_path) do |f| %>
  <div class="form-group">
    <div class="col-sm-2 control-label">
        <%= f.label :heading %>
    </div>
    <div class="col-sm-10">
      <%= f.text_field :heading, class: 'form-control', placeholder: "Enter Heading for Project Log" %>
    </div>
  </div>
 
   <div class="form-group">
    <div class="col-sm-2 control-label">
        <%= f.label :details %>
    </div>
    <div class="col-sm-10">
      <%= f.text_area :details, class: 'form-control', placeholder: "Enter Details" %>
    </div>
  </div>

 
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <%= f.submit class: 'btn btn-success btn-lg' %>
    </div>
  </div>
<% end %>